generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Trip {
  id          String   @id @default(cuid())
  title       String   @unique
  location    String
  description String
  from        DateTime
  to          DateTime
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
